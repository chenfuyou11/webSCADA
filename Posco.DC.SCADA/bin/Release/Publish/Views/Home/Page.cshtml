
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
    <title>Page</title>
    <link href="~/Content/ListLayui/static/admin/layui/css/layui.css" rel="stylesheet" />
    <link href="~/Content/ListLayui/static/admin/css/admin.css" rel="stylesheet" />
    <link href="~/Content/NewFolder1/Analysis.css" rel="stylesheet" />
    <script src="~/Content/ListLayui/static/admin/layui/layui.js"></script>
    <script src="~/Content/ListLayui/static/admin/js/common.js"></script>
</head>
<body>
    <!-- 页面权限管理 -->
    <div class="page-content-wrap">
        <div class="layui-form" id="table-page">
            <table class="layui-table" lay-skin="line">
                <colgroup>
                    <col width="20">
                    <col>
                    <col>
                    <col>
                </colgroup>
                <thead>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <th class="hidden-xs ">页面</th>
                        <th class="">菜单页面</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs">压缩空气系统能效分析</td>
                        <td class="classMenu">
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">压缩空气系统状态
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">空压站系统状态
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">空压机状态
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">趋势分析
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">统计分析
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">优化指导
                        </td>
                    </tr>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs ">智能控制设定</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs ">智能运维</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs ">能源报表</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs ">系统管理</td>
                        <td class="classMenu">
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">设备基础信息
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">管理员及权限信息
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">运维阈值
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">能效报警阈值
                        </td>

                    </tr>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs ">报警记录</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs ">操作日志</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td><input type="checkbox" name="" lay-skin="primary" data-id="1"></td>
                        <td class="hidden-xs ">后台管理</td>
                        <td class="classMenu">
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">客户列表
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">人员列表
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">工厂列表
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">空压站列表
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">区域列表
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">空压设备列表
                            <input type="checkbox" name="" lay-skin="checkRight" data-id="1">监测仪表列表
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <button style="padding: 0 40px; margin-left: 40px;" class="layui-btn layui-btn-normal" lay-submit lay-filter="emailInfo">保存</button>
    <button style="padding: 0 40px; margin-left: 40px; background-color: #666" class="layui-btn layui-btn-normal" lay-submit lay-filter="emailInfo">返回</button>
</body>
</html>
